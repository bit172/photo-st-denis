{
  "$schema": "./node_modules/nx/schemas/nx-schema.json",
  "defaultBase": "master",
  "plugins": [
    {
      "plugin": "@nx/js",
      "options": {
        "projectsCreatedWith": "npm"
      }
    }
  ],
  "namedInputs": {
    "default": [
      "{projectRoot}/**/*",
      "sharedGlobals"
    ],
    "production": [
      "default",
      "!{projectRoot}/**/?(*.)+(spec|test).[jt]s?(x)?(.snap)",
      "!{projectRoot}/tsconfig.spec.json",
      "!{projectRoot}/jest.config.[jt]s",
      "!{projectRoot}/.eslintrc.json",
      "!{projectRoot}/**/*.stories.@(js|jsx|ts|tsx|mdx)"
    ],
    "sharedGlobals": []
  },
  "targetDefaults": {
    "build": {
      "dependsOn": [
        "^build"
      ],
      "inputs": [
        "production",
        "^production"
      ],
      "outputs": [
        "{projectRoot}/dist"
      ],
      "cache": true
    },
    "start": {
      "dependsOn": [
        "^start"
      ],
      "cache": true
    },
    "dev": {
      "dependsOn": [
        "^dev"
      ],
      "cache": true
    },
    "test": {
      "dependsOn": [
        "^test"
      ],
      "cache": true
    },
    "lint": {
      "dependsOn": [
        "^lint"
      ],
      "cache": true
    },
    "format": {
      "dependsOn": [
        "^format"
      ],
      "cache": true
    },
    "start:dev": {
      "dependsOn": [
        "^start:dev"
      ],
      "cache": true
    },
    "start:debug": {
      "dependsOn": [
        "^start:debug"
      ],
      "cache": true
    },
    "start:prod": {
      "dependsOn": [
        "^start:prod"
      ],
      "cache": true
    },
    "clean": {
      "dependsOn": [
        "^clean"
      ],
      "cache": true
    },
    "test:watch": {
      "dependsOn": [
        "^test:watch"
      ],
      "cache": true
    },
    "test:cov": {
      "dependsOn": [
        "^test:cov"
      ],
      "cache": true
    },
    "test:debug": {
      "dependsOn": [
        "^test:debug"
      ],
      "cache": true
    },
    "test:e2e": {
      "dependsOn": [
        "^test:e2e"
      ],
      "cache": true
    }
  },
  "nxCloudId": "688c033ce83e13746288b001"
}
